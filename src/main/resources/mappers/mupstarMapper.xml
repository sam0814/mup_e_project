<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.mupe.mupstar.dao.MupStarMapper">
 	<select id="selectStarList" resultType="map">
 		SELECT
 			*
 		FROM
 			`star`
 	</select>
 	
 	<select id="selectPostListByUserId" parameterType="int" resultType="com.mupe.mupstar.domain.MupStar">
		SELECT
			`movieId`
			,`userId`
			,`startCount`
			,`createdAt`
			,`updatedAt`
		FROM
			`star`
		WHERE
			`userId` = #{userId}
		ORDER BY `movieId` DESC
	</select>
 </mapper>